#ifndef DDPARSERS_VERSION_H
#define DDPARSERS_VERSION_H 1

// define version macros for DDPARSERS
#define DDPARSERS_MAJOR_VERSION @DD4hep_VERSION_MAJOR@
#define DDPARSERS_MINOR_VERSION @DD4hep_VERSION_MINOR@

#define DDPARSERS_VERSION_GE(MAJV,MINV)  ( (  DDPARSERS_MAJOR_VERSION  > MAJV ) || \
                                        ( (DDPARSERS_MAJOR_VERSION == MAJV ) && \
                                          (DDPARSERS_MINOR_VERSION >= MINV ) ) )

#define DDPARSERS_VERSION_GT(MAJV,MINV)  ( (  DDPARSERS_MAJOR_VERSION  > MAJV ) || \
                                        ( (DDPARSERS_MAJOR_VERSION == MAJV ) && \
                                          (DDPARSERS_MINOR_VERSION >  MINV ) ) )


#endif // DDPARSERS_VERSION_H
